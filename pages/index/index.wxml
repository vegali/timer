<!--index.wxml-->
<view class="container">
  <view class="section home">
    <image class="logo" src="data:image/png;base64," />
    <view class="section__title" style="font-size: 40rpx;margin-bottom: 40rpx">· 时光如梭 · 了解自己 · 爱护自己 ·</view>
    <view class="section__title" style="color: #333;margin-top: 20rpx">请选择你的出生日期</view>
    <picker bindchange="bindPickerChange" value="{{typeIndex}}" range="{{dateType}}" class="dateType">
      <view class="picker">
        {{dateType[typeIndex]}}
      </view>
    </picker>
    <picker mode="date" value="{{date}}" start="1901-01-01" end="{{maxDate}}" bindchange="bindDateChange" class="dateChoose">
      <view class="picker">
        {{date}}
      </view>
    </picker>
    <navigator url="/pages/result/result" class="intoBtn">进入</navigator>
  </view>
</view>